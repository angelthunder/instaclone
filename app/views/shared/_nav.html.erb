<nav>
  <%= turbo_frame_tag "post-form" do %>
    <%= link_to "New Post", new_post_path %>
  <% end %>

  <% if user_signed_in? %>
    <%= link_to "Sign Out",
      destroy_user_session_path,
      data: { turbo_method: :delete } %>
  <% else %>
    <%= link_to "Sign In", new_user_session_path %>
  <% end %>
</nav>
